   "source": [
    "def isPalindrome(s):\n",
    "    return s == s[::-1]\n",
    "\n",
    "#s = \"malayalam\"\n",
    "s=(input(\"Enter a string  : \"))\n",
    "ans = isPalindrome(s)\n",
    " \n",
    "if ans:\n",
    "    print(f\"The string ‘{s}’ is a palindrome.\")\n",
    "else:\n",
    "    print(f\"The string ‘{s}’is not a palindrome\")"
   ]
